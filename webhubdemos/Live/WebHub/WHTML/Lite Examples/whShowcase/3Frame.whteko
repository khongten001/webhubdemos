<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE whteko PUBLIC "http://static.webhub.com/dtd/0214/whteko.dtd">
<whteko defaultlingvo="eng" designmode="code">

<whpagesettingslist id="a">
whSequence=off
bSeqHdr=false
bReqUl=false
level=3
levelKeyword=Frame
htmlFilename=3Frame.whteko
</whpagesettingslist>

<whmacros>
mcIconList=Earth-Earth,Fair-Fair,new-New,Sold-Sold,wow-Wow
mcCommandList=MATCH-The MATCH Command,PAGE-The PAGE Command,REQUIRES-The REQUIRES Command,INCLUDE-The INCLUDE Command,SENDFILE-The SENDFILE Command,SUMMARY-The SUMMARY Command
</whmacros>

<whpage pageid="FramePageA" desc="-Frame A">
<whpagesettings inherit="a">
</whpagesettings>
<HTML><HEAD><TITLE>Frame A</TITLE><HEAD>
<BODY>
(~Expires|~)
<DIV ALIGN=CENTER><CENTER>
<SMALL><b>Frame A</b></SMALL><br />
<table CELLPADDING=10 CELLSPACING=0 BORDER=0>
<tr style="vertical-align:top; text-align:center;">
<td>
<form method="post" accept-charset="UTF-8" action="(~ACTION|Showcase:FramePageB|target="FB"~)">
Please choose an Icon:<br />
<span style="font-size:small;">
(~INPUTSELECT|litFrameIcon, mcIconList,1~)<br />
(~inputsubmit|submit,Post to Frame B~)
</span>
</form>
</td>
<td>
<form method="post" accept-charset="UTF-8" action="(~ACTION|Showcase:FramePageC|target="FC"~)">
Please choose a Page:<br />
<span style="font-size:small;">
(~INPUTSELECT|litFramePage, mcCommandList,1~)<br />
(~inputsubmit|submit,Post to Frame C~)
</span>
</form>
</td>
</tr>
</table><br />
</CENTER></DIV></BODY></HTML>
</whpage>

<whpage pageid="FrameControl" desc="Frame Control Example">
<whpagesettings inherit="a">
</whpagesettings>
<HTML><HEAD><TITLE>Frame A</TITLE><HEAD>
<BODY><FONT FACE="Verdana,Arial">
