<?xml version="1.0" encoding="UTF-8"?>
<CleanEntrance xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <WebSites>
    <WebSite id="GooglePlusGSuite">
    	<Caption lingvo="en" value="GooglePlus"/>
        <Url value="https://plus.google.com/"/>
        <InputPattern value="individual"/><!-- google prompts for individual fields at a time -->
        <Fields>
	        <Field lingvo="en" prompt="email"    htmlAttr="id" htmlField="identifierId"/>
	        <Field lingvo="en" prompt="password" htmlAttr="id" htmlField="Passwd"/>
		<!--- for organization login, i.e. branded email with GSuite -->
        </Fields>
    </WebSite>
    <WebSite id="GooglePlus">
    	<Caption lingvo="en" value="GooglePlus"/>
        <Url value="https://plus.google.com/"/>
        <InputPattern value="individual"/>
        <Fields>
	        <Field lingvo="en" prompt="email"    htmlAttr="id" htmlField="identifierId"/>
	        <Field lingvo="en" prompt="password" htmlAttr="name" htmlField="password" parentElementID="password"/><!-- non-branded gmail account -->
        </Fields>
    </WebSite>
    <WebSite id="AWS">
    	<Caption lingvo="en" value="AWS Console"/>
        <Url value="https://console.aws.amazon.com/console/home"/>
        <InputPattern value="all"/>
        <Fields>
	        <Field lingvo="en" prompt="email" htmlAttr="id" htmlField="ap_email"/>
	        <Field lingvo="en" prompt="password" htmlAttr="id" htmlField="ap_password"/>
        </Fields>
    </WebSite>
    <WebSite id="BloggerGSuite">
    	<Caption lingvo="en" value="Blogger"/>
        <Url value="https://blogger.com/"/>
        <InputPattern value="individual"/>
        <Fields>
	        <Field lingvo="en" prompt="email"    htmlAttr="id" htmlField="identifierId"/>
	        <Field lingvo="en" prompt="password" htmlAttr="id" htmlField="Passwd"/>
        </Fields>
    </WebSite>
    <WebSite id="GMail">
    	<Caption lingvo="en" value="GMail"/>
        <Url value="https://mail.google.com/"/>
        <InputPattern value="individual"/>
        <Fields>
	        <Field lingvo="en" prompt="email"    htmlAttr="id" htmlField="identifierId"/>
	        <Field lingvo="en" prompt="password" htmlAttr="name" htmlField="password" parentElementID="password"/><!-- non-branded gmail account -->
        </Fields>
    </WebSite>
    <WebSite id="GoogleAdSense">
    	<Caption lingvo="en" value="Google AdSense"/>
        <Url value="https://www.google.com/adsense"/>
        <InputPattern value="individual"/>
        <Fields>
	        <Field lingvo="en" prompt="email"    htmlAttr="id" htmlField="identifierId"/>
	        <Field lingvo="en" prompt="password" htmlAttr="name" htmlField="password" parentElementID="password"/><!-- non-branded gmail account -->
        </Fields>
    </WebSite>
    <WebSite id="GoogleWebmaster">
    	<Caption lingvo="en" value="Google Webmaster Tools"/>
        <Url value="https://www.google.com/webmasters/tools/"/>
        <InputPattern value="individual"/>
        <Fields>
	        <Field lingvo="en" prompt="email"    htmlAttr="id" htmlField="identifierId"/>
	        <Field lingvo="en" prompt="password" htmlAttr="name" htmlField="password" parentElementID="password"/><!-- non-branded gmail account -->
        </Fields>
    </WebSite>
    <WebSite id="007">
    	<Caption lingvo="en" value="007 Names"/>
        <Url value="https://www.007names.com/"/>
        <InputPattern value="all"/>
        <Fields>
	        <Field lingvo="en" prompt="username" htmlAttr="id" htmlField="j_idt45:usernameTop"/>
	        <Field lingvo="en" prompt="password" htmlAttr="id" htmlField="j_idt45:passwordTop"/>
        </Fields>
    </WebSite>
  </WebSites>
</CleanEntrance>
