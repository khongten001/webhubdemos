</whdroplet>

<whdroplet name="chCheckLogin">
(~IFNOT|_bLoggedIn|(~MATCH|(~PageSetting.WHCPSecurePage~)=yes|(~SET|WHCPGoToPage=(~PageID~)~)(~BOUNCE|pgWHCPLogin~)~)~)
</whdroplet>

<whpage pageid="pgWHCPLogin" desc="Login">
<whpagesettings>
WHCPSecurePage=no
</whpagesettings>
(~chWHCPHeader~)
<form method="post" accept-charset="UTF-8" action="(~ACTIONR|pgWHCPLoginPoster~)">
<table border="0">
<tr><td>Username</td><td>(~INPUTTEXT|Username~)</td></tr>
<tr><td>Password</td><td>(~INPUTPASSWORD|Password~)</td></tr>
<tr><TD COLSPAN="2">(~INPUTSUBMIT|lButton,Login~)</td></tr>
</table>
</form>
(~chWHCPFooter~)
</whpage>

<whpage pageid="pgWHCPLoginPoster" desc="internal page">
<whpagesettings>
WHCPSecurePage=no
</whpagesettings>
<whprep>
(~wlWHCP.Execute|Username,Password,_bLoggedIn~)
(~IF|_bLoggedIn|(~BOUNCE|(~DynURL.LocSMU~)(~DynURL.cgiApp~):pgEnterWHCP:(~SessionID~)~)||(~BOUNCE|pgWHCPLogin~)~)
</whprep>
</whpage>

<whpage pageid="pgWHCPLogOut" desc="internal page">
<whprep>
(~CLEAR|_bLoggedIn~)
(~CLEAR|Username~)
(~CLEAR|Password~)
(~IFNOT|_bLoggedIn|(~BOUNCE|~)~)
</whprep>
</whpage>

<whdroplet name="chWHCPMenu">
(~IF|_bLoggedIn|
<TABLE BORDER="0" WIDTH="200">
<TR HEIGHT="1"><TD ROWSPAN="6" WIDTH="20">&nbsp;</td><td>&nbsp;</td></tr>

<tr><TD ALIGN="LEFT" VALIGN="TOP">
<FONT SIZE="+2">(~GO||MENU~)</FONT>
</td></tr>
<tr><TD ALIGN="LEFT" VALIGN="TOP">
<FONT SIZE="3">(~GO|pgApps|Applications~)</FONT>
</td></tr>
<tr><TD ALIGN="LEFT" VALIGN="TOP">
<FONT SIZE="3">(~GO|pgSettings|Settings~)</FONT>
</td></tr>
<tr><TD HEIGHT="16">&nbsp;</td></tr>
<tr><TD ALIGN="LEFT" VALIGN="TOP">
<FONT SIZE="3">(~JUMP|pgWHCPLogOut|Logout~)</FONT>
</td></tr>
</table>
~)
</whdroplet>

