<whteko defaultlingvo="eng" designmode="code">

<whpagesettingslist id="a">
whsequence=on
whSequenceUp=PGEXPIRE
whSequenceFinal=PGEXPIRE
bSeqHdr=true
bReqUL=false
level=4
levelKeyword=Expire
htmlFilename=4Expire.whteko
showcaseBranch=(~GO|pgHTML|Easier HTML~) &raquo; (~GO|pgWHMacros|WebHub Commands~) &raquo; (~GO|pgExpire|The EXPIRES Command~) &raquo;
showcaseBranchText=Easier HTML: WebHub Commands: The EXPIRES Command:
</whpagesettingslist>

<whpage pageid="pgENow" desc="Expire Now">
<whpagesettings inherit="a">
bNoMore=true
</whpagesettings>
<whprep>
(~EXPIRES|0~)
</whprep>
<whoutput>
(~drShowcaseGlobalHeader~)
<h2>Syntax</h2>
<code>
(~.EXPIRES|0~)
</code>

<h2>Explanation</h2>
This page expires now.
<h2>HEADERS</h2>
<span class="changing">(~HEADERS~)</span>
(~drShowcaseGlobalFooter~)
</whoutput>
</whpage>


<whpage pageid="pgETomorrow" desc="Tomorrow">
<whpagesettings inherit="a">
bNoMore=true
</whpagesettings>
<whprep>
(~EXPIRES|1~)
</whprep>
<whoutput>
(~drShowcaseGlobalHeader~)
<h2>Syntax</h2>
<code>
(~.EXPIRES|1~)
</code>

<h2>Explanation</h2>
This page expires tomorrow. The 1 indicates 1 day in the future.

<h2>HEADERS</h2>
<span class="changing">(~HEADERS~)</span>
(~drShowcaseGlobalFooter~)
</whoutput>
</whpage>

<whpage pageid="pgEDefault" desc="Default">
<whpagesettings inherit="a">
bNoMore=true
</whpagesettings>
(~EXPIRES|~)
(~drShowcaseGlobalHeader~)
<h2>Syntax</h2>
<code>
(~.EXPIRES|~)
</code>

<h2>Explanation</h2>
This is the default expiration. It is the same
as sending -1 explicitly.

<h2>HEADERS</h2>
<span class="changing">(~HEADERS~)</span>
(~drShowcaseGlobalFooter~)
</whpage>

<whpage pageid="pgEYesterday" desc="Yesterday">
